# Copy this file to .env and customize with your settings

# HostBill Configuration
HOSTBILL_URL=https://your-hostbill-instance.com
HOSTBILL_API_ID=your-api-id-here
HOSTBILL_API_KEY=your-api-key-here

# Crayon Cloud-IQ Configuration
CRAYON_CLIENT_ID=your-crayon-client-id
CRAYON_CLIENT_SECRET=your-crayon-client-secret
CRAYON_TENANT_ID=your-crayon-tenant-id
CRAYON_DYNAMIC_AUTH=false
CRAYON_USERNAME=your-username

# GenAI Configuration
ENABLE_GENAI=true
GENAI_API_KEY=your-openai-api-key
GENAI_MODEL=gpt-4

# Server Configuration
PORT=8000
SYNC_INTERVAL_MINUTES=60