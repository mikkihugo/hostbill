{
  "name": "@hostbill/cloud-iq",
  "version": "1.0.0",
  "exports": "./main.ts",
  "tasks": {
    "start": "deno run -A --env ./main.ts",
    "dev": "deno run -A --env --watch ./main.ts",
    "test": "deno test -A",
    "build": "echo 'Build not required for Deno'"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "strict": true,
    "allowUnreachableCode": false
  },
  "imports": {
    "$std/": "https://deno.land/std@0.230.0/"
  },
  "exclude": [
    "data/",
    "*.db"
  ]
}